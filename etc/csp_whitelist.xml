<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="devdocs-base" type="host">https://developer.adobe.com</value>
                <value id="magento" type="host">https://magento.com</value>
                <value id="fortis-sandbox" type="host">https://js.sandbox.fortis.tech</value>
                <value id="fortis" type="host">https://js.fortis.tech</value>
                <value id="fortis-elements-sandbox" type="host">https://elements.sandbox.fortis.tech</value>
                <value id="fortis-elements" type="host">https://elements.fortis.tech</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="devdocs" type="host">https://developer.adobe.com</value>
                <value id="fortis-elements-sandbox1" type="host">https://elements.sandbox.fortis.tech</value>
                <value id="fortis-elements1" type="host">https://elements.fortis.tech</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
